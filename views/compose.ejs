<%- include('partials/header') -%>
<h1>Compose</h1>
<form action="/compose" method="POST">
    
    <div class="form-group">
        <label for="article">Title</label>
        <input type="text" name="postTitle" class="form-control" autocomplete="off">

        <label for="">Post</label>
        <textarea name="postContent" rows="5" cols="30" class="form-control"></textarea>
    </div>

    <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>
<%- include('partials/footer') -%>